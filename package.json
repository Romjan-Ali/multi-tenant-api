  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/app.ts",
    "dev": "bun --watch run src/app.ts",
    "build": "bun install",
    "postinstall": "prisma generate",
    "studio": "bunx prisma studio",
